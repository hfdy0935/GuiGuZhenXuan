    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^8.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.0",
    "sass-loader": "^14.1.0",
    "scss-loader": "^0.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",

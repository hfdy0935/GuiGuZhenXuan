    "echarts-liquidfill": "^3.1.0",
    "element-plus": "^2.5.5",
    "html-loader": "^5.0.0",
    "markdown-loader": "^8.0.0",
    "marked": "^12.0.1",
    "mavon-editor": "^2.10.4",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia-plugin-persistedstate": "^3.2.1",
    "scss": "^0.2.4",
    "vditor": "^3.10.1",
